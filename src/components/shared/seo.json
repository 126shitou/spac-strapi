{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "seo"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "maxLength": 60,
      "required": true,
      "unique": true
    },
    "metaDescription": {
      "type": "text",
      "minLength": 50,
      "required": true
    },
    "metaImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "keywords": {
      "type": "string",
      "required": false,
      "regex": "[^,]+"
    },
    "metaRobots": {
      "type": "string"
    },
    "structuredData": {
      "type": "json"
    },
    "metaViewport": {
      "type": "string"
    },
    "canonicalURL": {
      "type": "string"
    }
  },
  "config": {}
}
