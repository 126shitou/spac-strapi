{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false,
      "required": true
    },
    "title": {
      "type": "string",
      "maxLength": 50,
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "minLength": 50,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "",
      "required": true,
      "regex": "^[a-z0-9-]+$"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::tinymce.tinymce",
      "required": true
    }
  }
}
